{
  "name": "dekburner",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "transpile-build": "tsc",
    "clean-all-outputs": "tsc --build tsconfig-glob.json --clean",
    "transpile-all": "tsc --build tsconfig-glob.json",
    "watch:transpile-all": "tsc --build --clean --project tsconfig-glob.json --watch --preserveWatchOutput",
    "watch:local-sync-all": "node dist/build/local-sync.js",
    "watch:remote": "bitburner-filesync"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "bitburner-filesync": "^1.2.0-beta4",
    "@types/jest": "^29.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
